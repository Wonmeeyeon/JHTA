user.registerUser = insert into withice_users (user_id, user_password, user_name, user_telephone, user_phone, user_sns_checkyn, user_email, user_gender, user_birth, user_location) \
					values (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)

